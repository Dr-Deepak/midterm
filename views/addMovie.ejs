<%-include partials/header.ejs %>
    <h1>Movie Details</h1>
    <form method="post">
      <div class="form-group">
        <label for= "title" class "col-sm-2 control-label">Title:</label>
        <input name="title" id="title" required/>
      </div>
      <div class="form-group">
        <label for= "studio" class "col-sm-2 control-label">Studio:</label>
        <input name="studio" id="studio" required/>
      </div>
        <div class="form-group">
          <label for= "year" class "col-sm-2 control-label">Year:</label>
          <input name="year" id="year" required type="number" min="1900" max="2016"/>
        </div>

          <button class="btn btn-success col-sm-offset-2">Save</button>
        </form>
<%-include partials/footer.ejs %>
